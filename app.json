{
  "expo": {
    "name": "Blank Template",
    "slug": "calculator_upplabs",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logos/app-logo.png",
    "splash": {
      "image": "./assets/images/logos/logo-with-text-white.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android" : {
      "package": "com.upplabs.calculator",
      "versionCode": 1,
      "permissions": []
    }
  }
}
